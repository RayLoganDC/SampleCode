/**
 * New node file
 */

var items = [
    "4;0,2,1"
];
for(var i=0; i<items[i].length; i++) {
		var ar = items[i].split(';');
		console.log (ar);
		var newAr = ar[1].split(',');
		console.log(newAr);
		newAr.sort();
		console.log(newAr);
		for(var i=0; i<newAr.length; i++) {
			if(newAr[i] == newAr[i+1]) {
                console.log(newAr[i]);
			}
		}
	}

